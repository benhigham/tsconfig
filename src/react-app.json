{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./browser.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "preserve",
    "noEmit": true
  }
}
